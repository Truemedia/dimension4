{
  "name": "dimension4",
  "version": "1.1.0",
  "description": "A node tool for dealing with advanced dimension plotting",
  "main": "node.js",
  "browser": "browser.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:node",
    "build:browser": "rollup src/main.js --file browser.js --format es",
    "build:node": "rollup src/main.js --file node.js --format cjs",
    "serve": "http-server ./",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Truemedia/dimension4.git"
  },
